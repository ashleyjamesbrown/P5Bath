//empty template sketch

void setup(){
  size(800,800); //size of sketch window
  background(0,0,0); //colour in R,G,B format
}

void draw(){
  fill(255,0,0); //colour to fill shape in R,G,B format
  ellipse(mouseX,mouseY, 100,100); //basic circle
}